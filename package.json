{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "-----DEV-----": "",
    "start:dev": "npm run build:dev && http-server -p 8100 dist/MyApp",
    "build:dev": "ng build MyApp",
    "postbuild:dev": "npm run build:webpack:dev",
    "build:webpack:dev": "rimraf ./dist/MyApp/service-worker.js && webpack --config ./webpack.dev.config.js --progress --colors && node ./workbox-build-inject.js",
    "-----PROD-----": "",
    "start:prod": "npm run build:prod && http-server -p 8100 dist/MyApp",
    "build:prod": "ng build MyApp --prod",
    "postbuild:prod": "npm run build:webpack:prod",
    "build:webpack:prod": "rimraf ./dist/MyApp/service-worker.js && webpack --config ./webpack.prod.config.js --progress --colors && node ./workbox-build-inject.js",
    "-----LOCAL-----": "",
    "start:local": "npm run build:webpack:local && ng serve",
    "build:webpack:local": "rimraf ./src/service-worker.js && webpack --config ./webpack.local.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.7",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/forms": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "workbox-background-sync": "5.0.0",
    "workbox-broadcast-update": "5.0.0",
    "workbox-cacheable-response": "5.0.0",
    "workbox-core": "5.0.0",
    "workbox-expiration": "5.0.0",
    "workbox-navigation-preload": "5.0.0",
    "workbox-precaching": "5.0.0",
    "workbox-routing": "5.0.0",
    "workbox-strategies": "5.0.0",
    "workbox-window": "5.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.15",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-loader": "6.2.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "workbox-build": "5.0.0"
  }
}
